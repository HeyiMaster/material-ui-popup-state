{
  "presets": [
    "flow",
    "react"
  ],
  "plugins": [
    "flow-react-proptypes"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {"targets": {"node": "current"}}]
      ],
      "plugins": ["istanbul"]
    },
    "es5": {
      "presets": [
        ["env", {"forceAllTransforms": true}]
      ],
      "plugins": [
        "transform-runtime",
        "transform-react-constant-elements"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements"
      ]
    }
  }
}
